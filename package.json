{
  "name": "free-parkings",
  "version": "1.0.0",
  "description": "How to use create-react-app with a custom Node API on Heroku",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "start": "node bin/www",
    "heroku-postbuild": "cd react-ui/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "express": "^4.16.4",
    "http-errors": "latest",
    "cookie-parser": "latest",
    "morgan": "latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goodwin64/free-parkings"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react",
    "parkings"
  ],
  "license": "MIT",
  "devDependencies": {}
}
